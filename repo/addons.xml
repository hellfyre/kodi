<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="service.knock" name="Knocking client" version="1.1" provider-name="Matthias Uschok">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.service" library="main.py" start="login"/>
  <extension point="xbmc.addon.metadata">
    <platform>linux</platform>
    <summary lang="en">Knocking client</summary>
    <description lang="en">Addon to open-knock servers running knockd.</description>
  </extension>
</addon>

<addon id="repository.uschok" name="Uschok's Kodi Repository" version="2" provider-name="Matthias Uschok">
  <extension point="xbmc.addon.repository" name="Uschok's Kodi Repository">
    <info compressed="false">https://uschok.de/kodi/isengard/addons.xml</info>
    <checksum>https://uschok.de/kodi/isengard/addons.xml.md5</checksum>
    <datadir zip="false">https://uschok.de/kodi/isengard</datadir>
  </extension>
  <extension point="xbmc.addon.metadata">
    <platform>all</platform>
    <summary lang="en">Uschok's Kodi Repository</summary>
  </extension>
</addon>
</addons>
